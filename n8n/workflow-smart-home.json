{
    "name": "SMP - Smart Home Fire Detection",
    "nodes": [
        {
            "parameters": {
                "httpMethod": "POST",
                "path": "smart-home",
                "responseMode": "onReceived",
                "options": {}
            },
            "id": "1a1a1a1a-0001-0001-0001-000000000001",
            "name": "Webhook",
            "type": "n8n-nodes-base.webhook",
            "typeVersion": 1,
            "position": [
                250,
                300
            ]
        },
        {
            "parameters": {
                "conditions": {
                    "options": {
                        "caseSensitive": true,
                        "leftValue": "",
                        "typeValidation": "strict"
                    },
                    "conditions": [
                        {
                            "id": "cond1",
                            "leftValue": "={{ $json.body.metrics.temp }}",
                            "rightValue": 50,
                            "operator": {
                                "type": "number",
                                "operation": "gt"
                            }
                        }
                    ],
                    "combinator": "or"
                }
            },
            "id": "1a1a1a1a-0001-0001-0001-000000000002",
            "name": "IF Temp > 50",
            "type": "n8n-nodes-base.if",
            "typeVersion": 2,
            "position": [
                480,
                300
            ]
        },
        {
            "parameters": {
                "jsCode": "const input = $input.first().json.body || $input.first().json;\nconst metrics = input.metrics;\nlet status = 'critical';\nlet message = 'ðŸ”¥ FIRE ALERT! Temperature: ' + metrics.temp + 'Â°C';\nif (metrics.smoke > 60) {\n  message += ' | Smoke: ' + metrics.smoke + '%';\n}\nreturn [{\n  json: {\n    deviceId: input.deviceId || 'dev_home_01',\n    domain: 'home',\n    metrics: metrics,\n    status: status,\n    message: message,\n    timestamp: Date.now()\n  }\n}];"
            },
            "id": "1a1a1a1a-0001-0001-0001-000000000003",
            "name": "Set Critical",
            "type": "n8n-nodes-base.code",
            "typeVersion": 2,
            "position": [
                700,
                200
            ]
        },
        {
            "parameters": {
                "jsCode": "const input = $input.first().json.body || $input.first().json;\nconst metrics = input.metrics;\nlet status = 'normal';\nlet message = 'All systems normal.';\nif (metrics.temp > 40 || metrics.smoke > 30) {\n  status = 'warning';\n  message = 'âš ï¸ Elevated temperature or smoke levels.';\n}\nreturn [{\n  json: {\n    deviceId: input.deviceId || 'dev_home_01',\n    domain: 'home',\n    metrics: metrics,\n    status: status,\n    message: message,\n    timestamp: Date.now()\n  }\n}];"
            },
            "id": "1a1a1a1a-0001-0001-0001-000000000004",
            "name": "Set Normal/Warning",
            "type": "n8n-nodes-base.code",
            "typeVersion": 2,
            "position": [
                700,
                420
            ]
        },
        {
            "parameters": {
                "method": "POST",
                "url": "http://host.docker.internal:3000/api/ingest",
                "sendBody": true,
                "specifyBody": "json",
                "jsonBody": "={{ JSON.stringify($json) }}",
                "options": {}
            },
            "id": "1a1a1a1a-0001-0001-0001-000000000005",
            "name": "POST to Core",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 4,
            "position": [
                940,
                300
            ]
        }
    ],
    "connections": {
        "Webhook": {
            "main": [
                [
                    {
                        "node": "IF Temp > 50",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "IF Temp > 50": {
            "main": [
                [
                    {
                        "node": "Set Critical",
                        "type": "main",
                        "index": 0
                    }
                ],
                [
                    {
                        "node": "Set Normal/Warning",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Set Critical": {
            "main": [
                [
                    {
                        "node": "POST to Core",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Set Normal/Warning": {
            "main": [
                [
                    {
                        "node": "POST to Core",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        }
    },
    "settings": {
        "executionOrder": "v1"
    },
    "pinData": {},
    "meta": {
        "instanceId": "smp-spla-demo"
    }
}